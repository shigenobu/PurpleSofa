<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net5.0;net6.0</TargetFrameworks>
        <LangVersion>9</LangVersion>
        <Description>Async tcp server and client library.</Description>
        <Authors>Shigenobu Furuta</Authors>
        <PackageId>PurpleSofa</PackageId>
        <PackageTags>async;non-blocking;tcp;tcp-server;tcp-client</PackageTags>
        <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
        <RepositoryType>git</RepositoryType>
        <RepositoryUrl>https://github.com/shigenobu/PurpleSofa.git</RepositoryUrl>
        <Nullable>enable</Nullable>
        <Version>0.0.4</Version>
    </PropertyGroup>

</Project>
